{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "1.0.0",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "private": false,
  "main": "dist/build.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "test:watch": "cross-env BABEL_ENV=test karma start test/karma.conf.js --auto-watch",
    "preversion": "git checkout master && git pull && npm run build ",
    "postversion": "npm publish && git push && git push --tags "
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "vue": "^2.5.11",
    "vue-resource": "^1.5.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^5.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    {{#less}}
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    {{/less}}
    "mocha": "^6.0.2",
    {{#mock}}
    "mockjs": "^1.0.1-beta3",
    {{/mock}}
    "sinon": "^7.3.0",
    "sinon-chai": "^3.3.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
